{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/function-component-definition": 0,
        "arrow-body-style": 0,
        "jsx-quotes": ["error", "prefer-single"],
        "comma-dangle": 0,
        "react/jsx-no-useless-fragment": 0,
        "react/prop-types": 0,
        "react/destructuring-assignment": 0,
        "jsx-a11y/label-has-associated-control": 0
    }
}
